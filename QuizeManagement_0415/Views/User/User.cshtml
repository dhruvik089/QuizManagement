@model List<QuizeManagement.Models.ViewModel.QuizzesModel>


@{ ViewBag.Title = "AdminDashboard";
    Layout = "~/Views/Shared/_LayoutUserPage.cshtml";
}

<div class="container mt-5">

    <table class="table table-striped table-bordered table-hover mt-5 ">

        <thead>

            <tr>

                <th>Title</th>

                <th>Description</th>

                <th>Action</th>

            </tr>

        </thead>

        <tbody>


            @if (Model != null)

            {

                foreach (var item in Model)

                {

                    <tr>


                        <td>@item.Title</td>

                        <td>@item.Description</td>

                        <td>

                            @Html.ActionLink("Start Quiz", "Question", "User", htmlAttributes: new { @class = "btn btn-primary" })

                        </td>

                    </tr>
                }
            }
        </tbody>

    </table>

</div>